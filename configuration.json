  "template": "${{CHANGELOG}}",
  "pr_template": "- **${{TITLE}}**",
  "label_extractor": [
    {
      "pattern": "(.+) (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": "(.+) (.+)",
      "target": "$2"
    }
  ],
  "duplicate_filter": {
    "pattern": ".+",
    "on_property": "title",
    "method": "match"
  },
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}
